		"@types/jsonwebtoken": "^8.5.1",
		"@types/mysql": "^2.15.18",
		"@types/node": "^14.14.31",
		"@typescript-eslint/eslint-plugin": "^4.15.0",
		"@typescript-eslint/parser": "^4.15.0",
		"eslint": "^7.19.0",
		"eslint-config-airbnb-base": "^14.2.1",
		"eslint-plugin-import": "2.22.1",
		"ts-node": "3.3.0",
		"ts-node-dev": "^1.1.1",
		"typescript": "3.3.3333"
	}
}
