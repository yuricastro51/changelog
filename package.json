	"bugs": {
		"url": "https://github.com/yuricastro51/changelog-api/issues"
	},
	"homepage": "https://github.com/yuricastro51/changelog-api#readme",
	"dependencies": {
		"axios": "^0.21.1",
		"cors": "^2.8.5",
		"dotenv": "^8.2.0",
		"express": "^4.15.4",
		"jsonwebtoken": "^8.5.1",
		"mysql2": "^2.2.5"
	},
	"devDependencies": {
		"@babel/cli": "^7.12.13",
		"@babel/core": "^7.12.13",
		"@babel/node": "^7.12.13",
		"@babel/preset-env": "^7.12.13",
		"@babel/preset-typescript": "^7.12.13",
		"@types/cors": "^2.8.9",
